package application;

import java.util.Locale;
import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		for(int i = 1; i <= 3; i++) {
			System.out.printf("%d° número: ", i);
		}

		sc.close();
	}

}
